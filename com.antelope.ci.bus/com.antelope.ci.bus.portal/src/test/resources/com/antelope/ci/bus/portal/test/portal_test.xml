<?xml version="1.0" encoding="UTF-8"?>
<portal>
	<extensions>
		<extension point="layout">
			<parts name="center.parts" embed="replace">
					<part name="second.menu" place="west" origin="part" />
					<place name="center">
						<parts>
							<part name="title" place="south" origin="part" />
							<part name="content" place="center" origin="part" />
							<part name="page" place="north" origin="global" />
						</parts>
					</place>
			</parts>
		</extension>
		<extension point="parts">
			<part name="top.memu.name" embed="append" sort="1">
				<content><![CDATA[${top.memu.name}]]></content>
			</part>
		</extension>
	</extensions>
</portal>
